<IfModule mod_rewrite.c>
    Options +SymLinksIfOwnerMatch
    RewriteEngine On
</IfModule>
 
<IfModule mod_rewrite.c>    
   
    RewriteCond %{REQUEST_URI} ^/(assets|css|js|images)
    RewriteRule ^assets/(.*)$ frontend/web/assets/$1 [L,DPI]
    RewriteRule ^css/(.*)$ frontend/web/css/$1 [L,DPI]
    RewriteRule ^js/(.*)$ frontend/web/js/$1 [L,DPI]
    RewriteRule ^images/(.*)$ frontend/web/images/$1 [L,DPI]
    RewriteRule ^(.*)$ frontend/web/$1 [L,DPI]
    RewriteCond %{REQUEST_URI} !^/(frontend|backend)/web/(assets|css|js)/
    RewriteCond %{REQUEST_URI} !index.php
    RewriteCond %{REQUEST_FILENAME} !-f [OR]
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^.*$ frontend/web/index.php
</IfModule>
